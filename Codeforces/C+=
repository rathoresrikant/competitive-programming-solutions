#include<iostream>
 
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int a,b,n,c,temp;
        cin>>a>>b>>n;
        c = 0;
        while(a<=n && b<=n)
        {
            if(a > b)
            {
                temp =a;
                a = b;
                b = temp;
            }
            a = a + b;
            c++;
 
        }
        cout<<c<<endl;
    }
}
