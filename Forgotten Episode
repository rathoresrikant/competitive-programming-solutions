#include <bits/stdc++.h>
using namespace std;
int main(){;
    int n,d;cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        arr[i]=-1;
    }
    for(int i=0;i<n;i++){
        cin>>d;
        arr[d-1]=0;
    }
    for(int i=0;i<n;i++){
        if(arr[i]==-1){cout<<(i+1)<<endl; return 0;}
    }
}
